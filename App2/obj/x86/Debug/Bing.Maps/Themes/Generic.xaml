                                                 <DataTemplate x:Key="VenueEntityListViewHeaderTemplate" >
                                                     <bm:AccessibleContainer x:Name="itemHeader" Margin="5, 0, 0, 15" Text="{Binding Name}" Template="{StaticResource VenueDirectoryListItemHeaderTextTemplate}"/>
                                                 </DataTemplate>
                                             </GroupStyle.HeaderTemplate>
                                         </GroupStyle>
                                     </ListView.GroupStyle>
                                 </ListView>
                                </Grid>
                            </Grid>
                        </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

