                        </bm:CustomListView>

                        <StackPanel
                            Grid.Row="2"  
                            Margin="20,15,24,15">
                            <bm:AccessibleContainer
                                x:Name="DisclaimerTextBlock"
                                Template="{StaticResource RouteDisclaimerTextTemplate}"
                                AcceptPrimaryAction="False"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
