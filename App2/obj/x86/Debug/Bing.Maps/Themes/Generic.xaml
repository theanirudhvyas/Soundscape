                        <x:Double x:Key="StepPushpinStrokeThickness">3</x:Double>
                        <Color x:Key="StepPushpinColor">White</Color>
                        <Color x:Key="StepPushpinStrokeColor">Black</Color>
                        <SolidColorBrush x:Key="StepPushpinColorBrush" Color="{StaticResource StepPushpinColor}"/>
                        <SolidColorBrush x:Key="StepPushpinStrokeColorBrush" Color="{StaticResource StepPushpinStrokeColor}"/>
                        <!--Transit pushpin resources-->
                        <!-- RoadPopup -->
                        <SolidColorBrush x:Key="RoadPopupFillNormalBrush" Color="{StaticResource ColorW}" />
                        <SolidColorBrush x:Key="RoadPopupTextNormalBrush" Color="{StaticResource ColorGB25}" />
                        <SolidColorBrush x:Key="RoadPopupFillPressedBrush" Color="{StaticResource ColorGB25}" />
                        <SolidColorBrush x:Key="RoadPopupTextPressedBrush" Color="{StaticResource ColorW}" />
                        <SolidColorBrush x:Key="RoadPopupMaskBrush" Color="{StaticResource ColorGB75}" />
                        <!--Waypoint pushpin template-->
                        <ControlTemplate TargetType="bm:Pushpin" x:Key="WaypointPushpinTemplate">
                            <Grid UseLayoutRounding="False">
                                <VisualStateManager.VisualStateGroups>
                                    <VisualStateGroup x:Name="CommonStates">
                                        <VisualState x:Name="PointerOver">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Background" Storyboard.TargetProperty="Fill">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource WaypointHoverColorBrush}"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Pressed">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Background" Storyboard.TargetProperty="Fill">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource WaypointTapColorBrush}"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Normal"/>
                                    </VisualStateGroup>
                                    <VisualStateGroup x:Name="SelectionStates">
                                        <VisualState x:Name="Selected">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="Background">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="SelectedBackground">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="WaypointText">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Unselected">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="Background">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="SelectedBackground">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="WaypointText">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                    </VisualStateGroup>
                                </VisualStateManager.VisualStateGroups>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                    <StackPanel Orientation="Horizontal"
                                                Grid.Row="0">
                                        <Grid VerticalAlignment="Top">
                                            <Rectangle x:Name="Background" 
                                                       Fill="{TemplateBinding Background}" 
                                                       Stroke="Black"
                                                       StrokeThickness="{StaticResource WaypointRectangleStrokeSize}"
                                                       Width="{StaticResource WaypointPushpinSize}" 
                                                       Height="{StaticResource WaypointPushpinSize}"/>
                                           <Rectangle x:Name="SelectedBackground"
                                                              Visibility="Collapsed"
                                                              Fill="{StaticResource SelectedWaypointColorBrush}"
                                                              HorizontalAlignment="Center"
                                                              Width="{StaticResource SelectedWaypointRectanglePushpinSize}"
                                                              Height="{StaticResource SelectedWaypointRectanglePushpinSize}" />
                                            <TextBlock Foreground="White"
                                                       x:Name="WaypointText"
                                                       HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       FontSize="20"
                                                       FontWeight="Bold"
                                                       Text="{TemplateBinding Text}">
                                            </TextBlock>
                                            
                                        </Grid>
                                        <Path Data="M30,0.25 L38.75,51.5 L29.75,29.75"
                                                 x:Name="Pointer"
                                                 Fill="Black"
                                                 Opacity="0.60000598430633545"
                                                 HorizontalAlignment="Left"
                                                 Height="{StaticResource WaypointPointerHeight}"
                                                 Margin="-1,0,0,-2"
                                                 Stretch="Fill"
                                                 UseLayoutRounding="False"
                                                 VerticalAlignment="Top"
                                                 Width="{StaticResource WaypointPointerWidth}"/>
                                    </StackPanel>
                                        <Grid   HorizontalAlignment="Right"
                                                Grid.Row="1"
                                                Margin="-7.5"
                                                Width="{StaticResource WaypointPointerBaseSize}"
                                                Height="{StaticResource WaypointPointerBaseSize}"
                                                UseLayoutRounding="False">
                                        <Ellipse  Fill="Black"
                                             StrokeThickness = "2"
                                             Stroke="White"
                                             Width="{StaticResource WaypointPointerBaseSize}"
                                             Height="{StaticResource WaypointPointerBaseSize}"/>
                                    </Grid>
                            </Grid>
                            
                        </ControlTemplate>
                        <!--Disambiguation pushpin template-->
                        <ControlTemplate TargetType="bm:Pushpin" x:Key="DisambiguationPushpinTemplate">
                            <Grid UseLayoutRounding="False">
                                <VisualStateManager.VisualStateGroups>
                                    <VisualStateGroup x:Name="CommonStates">
                                        <VisualState x:Name="PointerOver">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Background" Storyboard.TargetProperty="Fill">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource DisambiguationHoverColorBrush}"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Pressed">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Background" Storyboard.TargetProperty="Fill">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource DisambiguationTapColorBrush}"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Normal"/>
                                    </VisualStateGroup>
                                    <VisualStateGroup x:Name="SelectionStates">
                                        <VisualState x:Name="Selected">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="Background">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="SelectedBackground">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="DisambiguationText">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Unselected">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="Background">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="SelectedBackground">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="DisambiguationText">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                    </VisualStateGroup>
                                </VisualStateManager.VisualStateGroups>
                                    <Ellipse x:Name="Background"
                                                        Fill="{StaticResource DisambiguationPushpinColorBrush}"
                                                        StrokeThickness = "2"
                                                        Stroke="{StaticResource DisambiguationTextColorBrush}"
                                                        HorizontalAlignment="Center"
                                                        Width="{StaticResource DisambiguationPushpinSize}"
                                                        Height="{StaticResource DisambiguationPushpinSize}" />
                                    <Ellipse x:Name="SelectedBackground"
                                                        Visibility="Collapsed"
                                                        Fill="{StaticResource SelectedDisambiguationColorBrush}"
                                                        HorizontalAlignment="Center"
                                                        Width="{StaticResource SelectedDisambiguationPushpinSize}"
                                                        Height="{StaticResource SelectedDisambiguationPushpinSize}" />
                                    <TextBlock  Foreground="{StaticResource DisambiguationTextColorBrush}"
                                                x:Name="DisambiguationText"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}}">
                                    </TextBlock>
                            </Grid>
                        </ControlTemplate>
                        <!--Step Pushpin template-->
                        <ControlTemplate TargetType="bm:Pushpin" x:Key="StepPushpinTemplate">
                            <Grid x:Name="LayoutRoot" 
                              Width="{StaticResource StepPushpinSize}"
                              Height="{StaticResource StepPushpinSize}" 
                              UseLayoutRounding="False">
                                <Ellipse  Fill="{StaticResource StepPushpinColorBrush}" 
                                          StrokeThickness = "{StaticResource StepPushpinStrokeThickness}"
                                          Stroke="{StaticResource StepPushpinStrokeColorBrush}"
                                          Width="{StaticResource StepPushpinSize}" 
                                          Height="{StaticResource StepPushpinSize}"/>
                            </Grid>
                        </ControlTemplate>
                        <!--Transit bus/train pushpin-->
                        <ControlTemplate TargetType="bm:Pushpin" x:Key="TransitPushpinTemplate">
                            <Grid
                                x:Name="LayoutRoot"
                                MaxHeight="65"
                                MaxWidth="440"
                                CacheMode="BitmapCache"
                                UseLayoutRounding="True">
                                <Grid
                                    x:Name="FlyoutContents"
                                    Height="30"
                                    MinWidth="30"
                                    MaxWidth="440"
                                    Margin="15,0,0,35"
                                    VerticalAlignment="Bottom"
                                    HorizontalAlignment="Left">
                                    <Rectangle
                                        x:Name="FlyoutRectangle"
                                        Fill="{StaticResource RoadPopupFillNormalBrush}"
                                        Stroke="{StaticResource RoadPopupTextNormalBrush}"
                                        StrokeThickness="1"/>
                                    <Rectangle
                                        x:Name="SeparatorRectangle"
                                        Width="30"
                                        Stroke="{StaticResource RoadPopupTextNormalBrush}"
                                        StrokeThickness="1"
                                        HorizontalAlignment="Left"/>
                                    <Border
                                        x:Name="FlyoutBorder"
                                        FlowDirection="LeftToRight"
                                        BorderThickness="1">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border
                                                Grid.Column="0"
                                                Height="28"
                                                Width="28">
                                                <TextBlock
                                                    Foreground="Black"
                                                    Text="{TemplateBinding Text}"
                                                    FontFamily="{StaticResource SymbolFont}"
                                                    VerticalAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    Style="{StaticResource MediumText-R-None}"/>
                                            </Border>
                                            <Border
                                                x:Name="FlyoutStringBorder"
                                                Grid.Column="1"
                                                Height="28"
                                                BorderThickness="1,0,0,0">
                                                <TextBlock
                                                    Foreground="Black"
                                                    x:Name="FlyoutString"
                                                    Margin="5,0,5,0"
                                                    Text="{TemplateBinding Tag}"
                                                    TextTrimming="WordEllipsis"
                                                    VerticalAlignment="Center"
                                                    Style="{StaticResource MediumText-R-None}"/>
                                            </Border>
                                        </Grid>
                                    </Border>
                                    <Button
                                        x:Name="AutomationControl"
                                        VerticalAlignment="Stretch"
                                        HorizontalAlignment="Stretch"
                                        Style="{StaticResource POIButtonStyle}"/>
                                </Grid>
                                <Polyline
                                    x:Name="Beak"
                                    Points="15,0 0,36 25,0"
                                    Height="36"
                                    Width="25"
                                    Fill="{StaticResource RoadPopupFillNormalBrush}"
                                    Stroke="{StaticResource RoadPopupTextNormalBrush}"
                                    StrokeThickness="1"
                                    VerticalAlignment="Bottom"
                                    HorizontalAlignment="Left"
                                    RenderTransformOrigin="0.5,0.5">
                                    <Polyline.RenderTransform>
                                        <CompositeTransform/>
                                    </Polyline.RenderTransform>
                                </Polyline>
                            </Grid>
                        </ControlTemplate>
                        <!--Transit Walk pushpin-->
                        <ControlTemplate TargetType="bm:Pushpin" x:Key="TransitWalkPushpinTemplate">
                            <Grid
                                  x:Name="LayoutRoot"
                                  MaxHeight="65"
                                  MaxWidth="440"
                                  CacheMode="BitmapCache">
                                    <Grid
                                        x:Name="FlyoutContents"
                                        Height="30"
                                        MinWidth="30"
                                        MaxWidth="440"
                                        Margin="15,0,0,35"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Left">
                                        <Rectangle
                                            x:Name="FlyoutRectangle"
                                            Fill="{StaticResource RoadPopupFillNormalBrush}"
                                            Stroke="{StaticResource RoadPopupTextNormalBrush}"
                                            StrokeThickness="1"/>
                                        <Rectangle
                                            x:Name="SeparatorRectangle"
                                            Width="30"
                                            Stroke="{StaticResource RoadPopupTextNormalBrush}"
                                            StrokeThickness="1"
                                            HorizontalAlignment="Left"/>
                                        <Border
                                            x:Name="FlyoutBorder"
                                            FlowDirection="LeftToRight"
                                            BorderThickness="1">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Border
                                                    Grid.Column="0"
                                                    Height="28"
                                                    Width="28">
                                                    <TextBlock
                                                        Text="{StaticResource Walk}"
                                                        FontFamily="{StaticResource SymbolFont}"
                                                        VerticalAlignment="Center"
                                                        HorizontalAlignment="Center"
                                                        Foreground="Black"
                                                        Style="{StaticResource MediumText-R-None}"/>
                                                </Border>
                                            </Grid>
                                        </Border>
                                        <Button
                                            x:Name="AutomationControl"
                                            VerticalAlignment="Stretch"
                                            HorizontalAlignment="Stretch"
                                            AutomationProperties.Name="{Binding Path=AutomationName}"
                                            AutomationProperties.AutomationId="{Binding Path=AutomationID}"
                                            Style="{StaticResource POIButtonStyle}"/>
                                    </Grid>
                                    <Polyline
                                        x:Name="Beak"
                                        Points="15,0 0,36 25,0"
                                        Height="36"
                                        Width="25"
                                        Fill="{StaticResource RoadPopupFillNormalBrush}"
                                        Stroke="{StaticResource RoadPopupTextNormalBrush}"
                                        StrokeThickness="1"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Left"
                                        RenderTransformOrigin="0.5,0.5">
                                        <Polyline.RenderTransform>
                                            <CompositeTransform/>
                                        </Polyline.RenderTransform>
                                    </Polyline>
                                </Grid>
                        </ControlTemplate>
                    </bm:MapLayer.Resources>
                </bm:MapLayer>
                <!--Traffic pushpin layer-->
                <bm:MapLayer x:Name="trafficUILayer">
                    <bm:MapLayer.Resources>
                        <!-- Incident icon resources -->
                        <x:String x:Key="Unknown">&#xE371;</x:String>
                        <x:String x:Key="Acccident">&#xE367;</x:String>
                        <x:String x:Key="Congestion">&#xE371;</x:String>
                        <x:String x:Key="DisabledVehicle">&#xE371;</x:String>
                        <x:String x:Key="MassTransit">&#xE371;</x:String>
                        <x:String x:Key="Miscellaneous">&#xE371;</x:String>
                        <x:String x:Key="OtherNews">&#xE371;</x:String>
                        <x:String x:Key="PlannedEvent">&#xE371;</x:String>
                        <x:String x:Key="RoadHazard">&#xE371;</x:String>
                        <x:String x:Key="Construction">&#xE368;</x:String>
                        <x:String x:Key="Alert">&#xE371;</x:String>
                        <x:String x:Key="Weather">&#xE371;</x:String>
                        
                        <!--Pushpin template-->
                        <!--This is default template for traffic incidents-->
                        <ControlTemplate TargetType="bm:Pushpin" x:Key="TrafficIncidentPushpinDefaultTemplate">
                            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center">
                                <VisualStateManager.VisualStateGroups>
                                    <VisualStateGroup x:Name="SelectionStates">
                                        <VisualState x:Name="Unselected">
                                        </VisualState>
                                        <VisualState x:Name="Selected">
                                            <Storyboard>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="FlyoutPin">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="NormalPin">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                                </ObjectAnimationUsingKeyFrames>
                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(AutomationProperties.ItemStatus)" Storyboard.TargetName="AutomationControl">
                                                    <DiscreteObjectKeyFrame KeyTime="0" Value="Selected"/>
                                                </ObjectAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                    </VisualStateGroup>
                                </VisualStateManager.VisualStateGroups>

                                <Grid
                                    x:Name="Pin"
                                    Canvas.Left="-13"
                                    Canvas.Top="-13"
                                    Height="26"
                                    Width="26"
                                    CacheMode="BitmapCache">
                                    <Button
                                        x:Name="AutomationControl"
                                        Height="26"
                                        Width="26"
                                        IsHitTestVisible="False"
                                        Style="{StaticResource POIButtonStyle}"/>
                                    <Grid  
                                        x:Name="TrianglePin"
                                        Visibility="Visible">
                                        <Polyline 
						                                Points="0,22 24,22 12,0 0,22" 
						                                Fill="{StaticResource RoadTrafficPOIBorderNormalBrush}"
				                                    Stroke="{StaticResource  RoadTrafficPOIBorderNormalBrush}"  
						                                StrokeThickness="1" 
						                                StrokeLineJoin="Round" 
						                                StrokeEndLineCap="Round" 
						                                StrokeStartLineCap="Round"
						                                VerticalAlignment="Center"
						                                HorizontalAlignment="Center"/>
                                        <Polyline
                                            Margin="0,2,0,0"
                                            Points="0,18 20,18 10,0 0,18"
                                            Fill="{StaticResource RoadTrafficPOIFillNormalBrush}"
                                            Stroke="{StaticResource RoadTrafficPOIFillNormalBrush}"
                                            StrokeThickness="1"
                                            StrokeLineJoin="Round"
                                            StrokeEndLineCap="Round"
                                            StrokeStartLineCap="Round"
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Center"/>
                                    </Grid>

                                    <Border
                                        x:Name="PinFace"
                                        Height="26"
                                        Width="26"
                                        Background="Transparent">
                                        <Grid>
                                            <TextBlock
                                              Margin="0,6,0,0"
                                              FontSize="12"
                                              FontFamily="{StaticResource SymbolFont}"
                                              Text="{TemplateBinding Text}"
                                              Foreground="{StaticResource RoadTrafficPOITextNormalBrush}"
                                              VerticalAlignment="Center"
                                              HorizontalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                </Grid>
                                <Grid
                                    x:Name="FlyoutPin"
                                    Height="12"
                                    Width="12"
                                    Visibility="Collapsed"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center">
                                    <Ellipse
                                        Height="12"
                                        Width="12"
                                        Fill="{StaticResource POIMaskBrush}"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"/>
                                    <Ellipse
                                        Height="8"
                                        Width="8"
                                        Fill="{StaticResource DarkPopupFillNormalBrush}"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"/>
                                </Grid>
                            </Canvas>
                        </ControlTemplate>
                        <!--Override the default above if different pushpin should be displayed per incidenttype-->
                        <!--Accident-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="AcccidentPushpinTemplate"> 
                        </ControlTemplate-->
                        <!--Disabled Vehicle-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="DisabledVehiclePushpinTemplate">
                        </ControlTemplate-->
                        <!--Mass Transit-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="MassTransitPushpinTemplate">
                        </ControlTemplate-->
                        <!--Miscellaneous-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="MiscellaneousPushpinTemplate">
                        </ControlTemplate-->
                        <!--Other News-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="OtherNewsPushpinTemplate">
                        </ControlTemplate-->
                        <!--Planned Event-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="PlannedEventPushpinTemplate">
                        </ControlTemplate-->
                        <!--Road Hazard-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="RoadHazardPushpinTemplate">
                        </ControlTemplate-->
                        <!--Construction-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="ConstructionPushpinTemplate">
                        </ControlTemplate-->
                        <!--Alert-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="AlertPushpinTemplate"> 
                        </ControlTemplate-->
                        <!--Weather-->
                        <!--ControlTemplate TargetType="bm:Pushpin" x:Key="WeatherPushpinTemplate">
                        </ControlTemplate-->
                    </bm:MapLayer.Resources>
                </bm:MapLayer>
                <bm:MapLayer x:Name="internalUILayer">
                    <bm:MapLayer.Children>
                        <bm:AccessibleContainer 
                            x:Name="popup" 
                            Visibility="Collapsed"
                            Template="{StaticResource PopupTemplate}"/>
                        <bm:AccessibleContainer 
                            x:Name="infobox"  
                            Visibility="Collapsed"
                            Template="{StaticResource InfoboxTemplate}"/>
                    </bm:MapLayer.Children>
                </bm:MapLayer>
            </Grid>
            <Grid x:Name="mapForeground">
                <StackPanel 
                    x:Name="bottomLeftContainer" 
                    Margin="0,0,0,10"
                    Orientation="Vertical"
                    VerticalAlignment="Bottom" 
                    HorizontalAlignment="Left">
                    <bm:AccessibleContainer 
                        x:Name="logoElement" 
                        IsHitTestVisible="False" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False"
                        Template="{StaticResource LogoElementTemplate}"/>
                </StackPanel>
                <StackPanel
                    x:Name="bottomRightContainer" 
                    Margin="0,0,0,10"
                    Orientation="Vertical"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Right">
                    <bm:AccessibleContainer 
                        x:Name="scalebarElement" 
                        Margin="10,10,10,0"
                        IsHitTestVisible="False" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False"
                        Template="{StaticResource ScalebarTemplate}"/>
                    <bm:AccessibleContainer 
                        x:Name="copyrightElement" 
                        Margin="10,10,10,0"
                        IsHitTestVisible="False"  
                        IsTabStop="False" 
                        AcceptPrimaryAction="False"
                        Template="{StaticResource CopyrightTemplate}"/>
                </StackPanel>
                <StackPanel 
                    x:Name="topLeftContainer" 
                    Orientation="Vertical"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Left"/>
                <StackPanel 
                    x:Name="topRightContainer" 
                    Orientation="Vertical"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Right"/>
                <Grid
                    x:Name="navControlContainer" 
                    VerticalAlignment="Top">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <bm:AccessibleContainer  
                        x:Name="breadcrumb"
                        Grid.Row="0"
                        Height="{StaticResource ControlHeight}" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False" 
                        Template="{StaticResource BreadcrumbTemplate}"
                        HorizontalAlignment="Stretch"/>
                    <bm:AccessibleContainer  
                        x:Name="navigationControl" 
                        Grid.Row="1"
                        Margin="10,10,10,0" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False" 
                        Template="{StaticResource NavigationControlTemplate}" 
                        HorizontalAlignment="Stretch"/>
                    <Grid  
                        Grid.Row="1" 
                        Margin="0,10,0,0" 
                        VerticalAlignment="Top"
                        HorizontalAlignment="Center">
                        <bm:AccessibleContainer 
                            x:Name="floorControl" 
                            Visibility="Collapsed"  
                            IsTabStop="False" 
                            AcceptPrimaryAction="False" 
                            Template="{StaticResource VenueFloorControlTemplate}"/>
                    </Grid>
                 </Grid>
            </Grid>
            <Grid x:Name="messagePanel">
                <bm:AccessibleContainer 
                    x:Name="invalidCredentialMessage" 
                    Visibility="Collapsed" 
                    Template="{StaticResource InvalidCredentialsMessageTemplate}"/>
            </Grid>
        </Grid>
    </ControlTemplate>
</ResourceDictionary>

