    <!-- COPYRIGHT -->
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="CopyrightTemplate">
        <Grid 
            x:Name="LayoutRoot" 
            UseLayoutRounding="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MapTypeStates">
                    <VisualState x:Name="Road">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyrightShadow" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyright" To="Black" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="AerialBE">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyrightShadow" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyright" To="White" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="MapSizeStates">
                    <VisualState x:Name="Small">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyright">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SmallCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyrightShadow">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SmallCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Medium">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyright">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource MediumCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyrightShadow">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource MediumCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Large">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyright">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource LargeCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyrightShadow">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource LargeCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <TextBlock
                x:Name="copyrightShadow" 
                VerticalAlignment="Bottom"
                HorizontalAlignment="Right"/>
            <TextBlock 
                x:Name="copyright" 
                Margin="0,0,1,1"
                VerticalAlignment="Bottom" 
                HorizontalAlignment="Right"/>
        </Grid>
    </ControlTemplate>

    <!-- SCALE -->
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="ScalebarTemplate">
        <Grid 
            x:Name="LayoutRoot" 
            UseLayoutRounding="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MapTypeStates">
                    <VisualState x:Name="Road">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialTextShadow" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialText" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricTextShadow" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricText" To="Black" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="AerialBE">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialTextShadow" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialText" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricTextShadow" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricText" To="White" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <StackPanel 
                x:Name="scaleBarContainer" 
                Orientation="Horizontal">
                <StackPanel
                    Margin="0,0,2,0"
                    Orientation="Vertical">
                    <Grid>
                        <TextBlock
                            x:Name="imperialTextShadow" 
                            TextWrapping="Wrap" 
                            Text="250 miles" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                        <TextBlock 
                            x:Name="imperialText" 
                            Margin="0,0,1,1" 
                            TextWrapping="Wrap" 
                            Text="250 miles" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                    </Grid>
                    <Rectangle
                        x:Name="imperialScaleBar" 
                        Fill="#FFF4F4F5" 
                        Height="5" 
                        Stroke="#FF888888" 
                        VerticalAlignment="Bottom" 
                        Width="100" 
                        HorizontalAlignment="Right"/>
                </StackPanel>
                <StackPanel
                    Orientation="Vertical">
                    <Grid>
                        <TextBlock 
                            x:Name="metricTextShadow" 
                            TextWrapping="Wrap" 
                            Text="500 km" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                        <TextBlock 
                            x:Name="metricText" 
                            Margin="0,0,1,1" 
                            TextWrapping="Wrap" 
                            Text="500 km" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                    </Grid>
                    <Rectangle
                        x:Name="metricScaleBar" 
                        Fill="#FFF4F4F5" 
                        Height="5"  
                        Stroke="#FF888888" 
                        Width="100" 
                        HorizontalAlignment="Right"/>
                </StackPanel>
            </StackPanel>
        </Grid>
    </ControlTemplate>
    
