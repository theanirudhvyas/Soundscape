    <!-- BREADCRUMB UI -->
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="BreadcrumbTextBlockTemplate">
        <Grid x:Name="LayoutRoot">
            <Grid.Resources>
                <SolidColorBrush x:Name="TextColorBrush" x:Key="TextColorBrush" Color="{StaticResource ForegroundFillColorRoad}"/>
                <SolidColorBrush x:Name="HoverColorBrush" x:Key="HoverColorBrush" Color="{StaticResource HoverColorRoad}"/>
                <SolidColorBrush x:Name="HoverIconColorBrush" x:Key="HoverIconColorBrush" Color="{StaticResource HoverIconColorRoad}"/>
                <SolidColorBrush x:Name="TapColorBrush" x:Key="TapColorBrush" Color="{StaticResource TapColorRoad}"/>
                <SolidColorBrush x:Name="TapIconColorBrush" x:Key="TapIconColorBrush" Color="{StaticResource TapIconColorRoad}"/>
                <SolidColorBrush x:Name="PushColorBrush" x:Key="PushColorBrush" Color="{StaticResource PushColorRoad}"/>
            </Grid.Resources>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MapTypeStates">
                    <VisualState x:Name="Road">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TextColorBrush" To="{StaticResource ForegroundFillColorRoad}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="HoverColorBrush" To="{StaticResource HoverColorRoad}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="HoverIconColorBrush" To="{StaticResource HoverIconColorRoad}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TapColorBrush" To="{StaticResource TapColorRoad}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TapIconColorBrush" To="{StaticResource TapIconColorRoad}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="PushColorBrush" To="{StaticResource PushColorRoad}" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="AerialBE">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TextColorBrush" To="{StaticResource ForegroundFillColorAerialBE}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="HoverColorBrush" To="{StaticResource HoverColorAerialBE}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="HoverIconColorBrush" To="{StaticResource HoverIconColorAerialBE}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TapColorBrush" To="{StaticResource TapColorAerialBE}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TapIconColorBrush" To="{StaticResource TapIconColorAerialBE}" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="PushColorBrush" To="{StaticResource PushColorAerialBE}" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="PointerOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Background" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource HoverColorBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="textblock" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource HoverIconColorBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Background" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource TapColorBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="textblock" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource TapIconColorBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                </VisualState>
                    <VisualState x:Name="Normal"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Rectangle 
                x:Name="Background"
                Fill="Transparent"/>
            <TextBlock 
                x:Name="textblock" 
                Margin="10,0,10,0"
                Text="{TemplateBinding Text}"
                Foreground="{StaticResource TextColorBrush}"
                VerticalAlignment="Center"
                Style="{StaticResource MediumText-R-None}" />
        </Grid>
    </ControlTemplate>

