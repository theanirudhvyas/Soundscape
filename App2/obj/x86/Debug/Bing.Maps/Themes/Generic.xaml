    <Style TargetType="dd:DirectionsInput">
        <Setter Property="MinHeight" Value="86"/>
        <Setter  Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dd:DirectionsInput">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="RouteOptions">
                                <VisualState x:Name="Transit">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Visibility)" Storyboard.TargetName="DirectionsOptionsGroup">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="TransitOptionsGroup">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Driving">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Visibility)" Storyboard.TargetName="DirectionsOptionsGroup">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="TransitOptionsGroup">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="None">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Visibility)" Storyboard.TargetName="DirectionsOptionsGroup">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="TransitOptionsGroup">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <!-- Controls inside ScrollViewer seems not to be accessible to UI Automation-->
                        <!--ScrollViewer
                              VerticalScrollBarVisibility="Disabled"
                              VerticalScrollMode="Disabled"
                              HorizontalScrollBarVisibility="Disabled"
                              HorizontalScrollMode="Disabled"
                              ZoomMode="Disabled"
                              IsTabStop="True"-->
                        <Grid x:Name="RootElement">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.Resources>
                                <CollectionViewSource x:Name="TransitModesViewSource" />
                                <!-- Directions Options Toggle Button -->
                                <SolidColorBrush x:Key="DirectionsOptionsArrowFillBrush" Color="{StaticResource ColorW}" />
                                <SolidColorBrush x:Key="DirectionsOptionsArrowPointerOverFillBrush" Color="{StaticResource ColorGB75}" />

                                <Style x:Key="DirectionsCheckBoxStyle" TargetType="CheckBox">
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="CheckBox">
                                                <Grid Background="Transparent">
                                                    <VisualStateManager.VisualStateGroups>
                                                        <VisualStateGroup x:Name="CommonStates">
                                                            <VisualState x:Name="Normal"/>
                                                            <VisualState x:Name="PointerOver">
                                                                <Storyboard>
                                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ArrowPolyline" Storyboard.TargetProperty="Fill">
                                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource DirectionsOptionsArrowPointerOverFillBrush}"/>
                                                                    </ObjectAnimationUsingKeyFrames>
                                                                </Storyboard>
                                                            </VisualState>
                                                            <VisualState x:Name="Pressed">
                                                                <Storyboard>
                                                                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ArrowPolyline" Storyboard.TargetProperty="Fill">
                                                                        <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource DirectionsOptionsArrowPointerOverFillBrush}"/>
                                                                    </ObjectAnimationUsingKeyFrames>
                                                                </Storyboard>
                                                            </VisualState>
                                                            <VisualState x:Name="Disabled"/>
                                                        </VisualStateGroup>
                                                        <VisualStateGroup x:Name="CheckStates">
                                                            <VisualState x:Name="Checked">
                                                                <Storyboard>
                                                                    <DoubleAnimation Duration="0:0:0.5" To="90" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)" Storyboard.TargetName="ArrowPolyline">
                                                                        <DoubleAnimation.EasingFunction>
                                                                            <QuarticEase EasingMode="EaseInOut"/>
                                                                        </DoubleAnimation.EasingFunction>
                                                                    </DoubleAnimation>
                                                                </Storyboard>
                                                            </VisualState>
                                                            <VisualState x:Name="Unchecked">
                                                                <Storyboard>
                                                                    <DoubleAnimation Duration="0:0:0.5" To="0" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)" Storyboard.TargetName="ArrowPolyline">
                                                                        <DoubleAnimation.EasingFunction>
                                                                            <QuarticEase EasingMode="EaseInOut"/>
                                                                        </DoubleAnimation.EasingFunction>
                                                                    </DoubleAnimation>
                                                                </Storyboard>
                                                            </VisualState>
                                                            <VisualState x:Name="Indeterminate"/>
                                                        </VisualStateGroup>
                                                        <VisualStateGroup x:Name="FocusStates">
                                                            <VisualState x:Name="Focused">
                                                                <Storyboard>
                                                                    <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="FocusVisualWhite"/>
                                                                    <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="FocusVisualBlack"/>
                                                                </Storyboard>
                                                            </VisualState>
                                                            <VisualState x:Name="Unfocused"/>
                                                            <VisualState x:Name="PointerFocused"/>
                                                        </VisualStateGroup>
                                                    </VisualStateManager.VisualStateGroups>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="60"/>
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Polyline
                                                            x:Name="ArrowPolyline"
                                                            Grid.Column="0"
                                                                            Margin="40,0,0,0"
                                                            Points="0,0 0,16 8,8"
                                                            Fill="{StaticResource DirectionsOptionsArrowFillBrush}"
                                                            VerticalAlignment="Center"
                                                            HorizontalAlignment="Left"
                                                            RenderTransformOrigin="0.5,0.5">
                                                            <Polyline.RenderTransform>
                                                                <CompositeTransform/>
                                                            </Polyline.RenderTransform>
                                                        </Polyline>
                                                        <TextBlock
                                                        Grid.Column="1"
                                                        Margin="0,0,40,0"
                                                        Text="{TemplateBinding Content}"
                                                        TextTrimming="WordEllipsis"
                                                        VerticalAlignment="Center"
                                                        HorizontalAlignment="Left"
                                                        Style="{StaticResource MediumText-R-White}"/>
                                                    </Grid>
                                                    <Rectangle x:Name="FocusVisualWhite" IsHitTestVisible="False" Opacity="0" StrokeDashOffset="1.5" StrokeEndLineCap="Square" Stroke="{StaticResource FocusVisualWhiteStrokeThemeBrush}" StrokeDashArray="1,1"/>
                                                    <Rectangle x:Name="FocusVisualBlack" IsHitTestVisible="False" Opacity="0" StrokeDashOffset="0.5" StrokeEndLineCap="Square" Stroke="{StaticResource FocusVisualBlackStrokeThemeBrush}" StrokeDashArray="1,1"/>
                                                </Grid>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Grid.Resources>

                            <StackPanel
                                Grid.Row="0"
                                Margin="40,0,40,0"
                                Orientation="Vertical">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="15"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="15"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Border
                                    Grid.Column="0"
                                    Background="{StaticResource RoadStartPOIFillNormalBrush}"
                                    x:Name="StartLocationBorder">
                                        <TextBlock
                                        Text="A"
                                        Margin="-1,0,0,0"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        Foreground="{StaticResource RoadStartPOITextNormalBrush}"
                                        Style="{StaticResource ExtraSmallText-B-None}"/>
                                    </Border>
                                    <Border
                                    x:Name="OriginCluster"
                                    Grid.Column="1"
                                    Height="32">
                                        <Grid Background="{StaticResource InputBoxFillBrush}">
                                            <TextBox
                                            x:Name="OriginField"
                                            TextWrapping="NoWrap"
                                            TabIndex="1"
                                            Style="{StaticResource InputBox}">
                                                <TextBox.InputScope>
                                                    <InputScope>
                                                        <InputScope.Names>
                                                            <InputScopeName NameValue="Search"/>
                                                        </InputScope.Names>
                                                    </InputScope>
                                                </TextBox.InputScope>
                                            </TextBox>
                                            <TextBlock
                                            x:Name="OriginWatermak"
                                            Text="From"
                                            Style="{StaticResource watermarkText}"/>
                                            <!-- Location Me feature is not included yet-->
                                            <!--Button
                                                x:Name="CurrentLocOrigin"
                                                AutomationProperties.Name="My location"
                                                AutomationProperties.HelpText="Use my current location"
                                                ToolTipService.ToolTip="Use my current location"
                                                TabIndex="2" Style="{StaticResource CurLocButtonStyle}"
                                                HorizontalAlignment="Right"/-->
                                        </Grid>
                                    </Border>
                                    <Button
                                        x:Name="SwapDirections"
                                        Grid.Column="2"
                                        TabIndex="6"
                                        Content="&#xE008;"
                                        Style="{StaticResource ActionButtonStyle}"
                                        AutomationProperties.AutomationId="SwapDirections"
                                        AutomationProperties.ItemType="Swap Button"/>
                                    <!--Currently no suggestion feature-->
                                    <!--ListBox
                                        x:Name="OriginSuggestionListBox"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="2"
                                        Grid.Row="1"
                       
                                        Visibility="Collapsed"
                                        ItemTemplate="{StaticResource SuggestionListBoxItemTemplate}"
                                        ItemContainerStyle="{StaticResource SuggestionListBoxItemStyle}" /-->
                                    <Border
                                        Grid.Column="0"
                                        Grid.Row="3"
                                         x:Name="EndLocationBorder"
                                        Background="{StaticResource RoadEndPOIFillNormalBrush}">
                                        <TextBlock
                                        Text="B"
                                        Margin="-1,0,0,0"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        Foreground="{StaticResource RoadEndPOITextNormalBrush}"
                                        Style="{StaticResource ExtraSmallText-B-None}"/>
                                    </Border>
                                    <Border
                                        x:Name="DestinationCluster"
                                        Grid.Column="1"
                                        Grid.Row="3"
                                        Height="32">
                                        <Grid Background="{StaticResource InputBoxFillBrush}">
                                            <TextBox
                                            x:Name="DestinationField"
                                            TabIndex="3"
                                            Style="{StaticResource InputBox}">
                                                <TextBox.InputScope>
                                                    <InputScope>
                                                        <InputScope.Names>
                                                            <InputScopeName NameValue="Search"/>
                                                        </InputScope.Names>
                                                    </InputScope>
                                                </TextBox.InputScope>
                                            </TextBox>
                                            <TextBlock
                                            Text="To"
                                            x:Name="DestinationWatermark"
                                            Style="{StaticResource watermarkText}"/>
                                            <!-- Location Me feature is not included yet-->
                                            <!--Button
                                                x:Name="CurrentLocDestination"
                                                AutomationProperties.Name="My Location"
                                                AutomationProperties.HelpText="Use my current location"
                                                ToolTipService.ToolTip="Use my current location"
                                                TabIndex="4"
                                                HorizontalAlignment="Right"
                                                Style="{StaticResource CurLocButtonStyle}"/-->
                                        </Grid>
                                    </Border>
                                    <Button
                                        x:Name="DoDirectionsButton"
                                        Grid.Column="2"
                                        Grid.Row="3"
                                        Content="&#x203A;"
                                        TabIndex="5"
                                        AutomationProperties.AutomationId="DoDirectionsButton"
                                        AutomationProperties.ItemType="Go Button"
                                        Style="{StaticResource ActionButtonStyle}"/>
                                    <!--Currently no suggestion feature-->
                                    <!--ListBox
                                        x:Name="DestinationSuggestionListBox"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="2"
                                        Grid.Row="4"
                                        Visibility="Collapsed"
                                        ItemTemplate="{StaticResource SuggestionListBoxItemTemplate}"
                                        ItemContainerStyle="{StaticResource SuggestionListBoxItemStyle}"/-->
                                </Grid>
                                <RichTextBlock
                                    x:Name="DirectionsErrorText"
                                    Margin="0,5,0,0"
                                    TextWrapping="Wrap"
                                    IsTextSelectionEnabled="True"
                                    Visibility="Collapsed"
                                    Style="{StaticResource MediumText-R-White-RichTextBlock}"/>
                            </StackPanel>

                            <StackPanel
                                Grid.Row="1"
                                Margin="40,15,40,15"
                                Orientation="Horizontal">
                                <RadioButton
                                    x:Name="DrivingButton"
                                    GroupName="DirectionsGroupMode"
                                    Margin="0,0,10,0"
                                    TabIndex="6"
                                    Content="&#xe005;"
                                    IsChecked="True"
                                    AutomationProperties.AutomationId="DrivingButton"
                                    Style="{StaticResource DirectionsButtonStyle}"/>
                                <RadioButton
                                    x:Name="TransitButton"
                                    GroupName="DirectionsGroupMode"
                                    Margin="0,0,10,0"
                                    TabIndex="7"
                                    Content="&#xe046;"
                                    IsChecked="False"
                                    AutomationProperties.AutomationId="TransitButton"
                                    Style="{StaticResource DirectionsButtonStyle}"/>
                                <RadioButton
                                    x:Name="WalkingButton"
                                    GroupName="DirectionsGroupMode"
                                    TabIndex="8"
                                    Content="&#xe006;"
                                    IsChecked="False"
                                    AutomationProperties.AutomationId="WalkingButton"
                                    Style="{StaticResource DirectionsButtonStyle}"/>
                            </StackPanel>

                            <CheckBox 
                                Grid.Row="2"
                                x:Name="OptionsCheckBox"
                                Height="40"
                                HorizontalAlignment="Stretch"
                                Style="{StaticResource DirectionsCheckBoxStyle}"/>
                            <!--ScrollViewer
                                Grid.Row="3"
                                HorizontalScrollBarVisibility="Disabled"
                                HorizontalScrollMode="Disabled"
                                VerticalScrollBarVisibility="Auto"
                                VerticalScrollMode="Enabled"
                                ZoomMode="Disabled"-->
                                <Grid 
                                    Margin="40,15,40,15" 
                                    Grid.Row="3">
                                    <StackPanel
                                         Visibility="Collapsed"
                                         x:Name="TransitOptionsGroup"
                                         HorizontalAlignment="Stretch"
                                         Orientation="Vertical">
                                        <ComboBox
                                            x:Name="TransitModeComboBox"
                                            AllowDrop="true"
                                            ItemsSource="{Binding Source={StaticResource TransitModesViewSource}}"
                                            HorizontalAlignment="Stretch"/>
                                        <ContentControl 
                                            x:Name="DatePicker"
                                            Margin="0,15,0,0"
                                            HorizontalAlignment="Stretch"
                                            HorizontalContentAlignment="Stretch">
                                            <ComboBox
                                                x:Name="DateComboBox"
                                                HorizontalAlignment="Stretch"/>
                                        </ContentControl>
                                        <ContentControl 
                                            x:Name="TimePicker" 
                                            Margin="0,15,0,0"
                                            HorizontalAlignment="Stretch"
                                            HorizontalContentAlignment="Stretch">
                                            <StackPanel
                                                x:Name="TransitTimePanel"
                                                Orientation="Horizontal">
                                                <ComboBox
                                                    x:Name="HourComboBox"
                                                    Width="81"/>
                                                <TextBlock
                                                    Text=":"
                                                    Margin="4,0,3,0"
                                                    VerticalAlignment="Center"
                                                    Style="{StaticResource MediumText-R-White}"/>
                                                <ComboBox
                                                    x:Name="MinuteComboBox"
                                                    Width="81"/>
                                                <ComboBox
                                                    x:Name="AMPMComboBox"
                                                    Width="81"
                                                    Margin="11,0,0,0"/>
                                            </StackPanel>
                                        </ContentControl>
                                        <!--We need to add this support if map app use our UI-->
                                        <!--StackPanel Orientation="Vertical">
                                        <controls:TransitOptionsJapan
                                            x:Name="TransitOptionsJapan"
                                            Visibility="{Binding Path=AppRegion, Converter={StaticResource JapanTransitOptionsVisibilityConverter}}"
                                            Margin="0,15,0,0"/>
                                    </StackPanel-->

                                    </StackPanel>

                                    <StackPanel
                                       Visibility="Collapsed"
                                       x:Name="DirectionsOptionsGroup"
                                       Orientation="Vertical">
                                        <Grid  x:Name="DirectionsOptionsControl"
                                            Width="Auto">
                                            <StackPanel
                                                Orientation="Vertical">
                                                <bm:AccessibleContainer
                                                    AcceptPrimaryAction="False"
                                                    x:Name="AvoidHighwaysText"
                                                    Template="{StaticResource DirectionsLabelTemplate}"/>
                                                <ToggleSwitch
                                                    x:Name="AvoidHighwaysOnOff"
                                                    TabIndex="8"/>
                                                <bm:AccessibleContainer
                                                    AcceptPrimaryAction="False"
                                                    Margin="0,10,0,0"
                                                    x:Name="AvoidTollsText"
                                                    Template="{StaticResource DirectionsLabelTemplate}"/>
                                                <ToggleSwitch
                                                    x:Name="AvoidTollsOnOff"
                                                    TabIndex="8"/>
                                            </StackPanel>
                                        </Grid>
                                    </StackPanel>
                                </Grid>
                            <!--/ScrollViewer-->
                        </Grid>
                        <!--/ScrollViewer-->
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="dd:RouteSummary">
        <Setter Property="MinHeight" Value="86"/>
        <Setter  Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dd:RouteSummary">
                    <Grid 
                        x:Name="LayoutRoot" 
                        Background="{StaticResource RouteSummaryBackgroundBrush}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="RouteMode">
                                <VisualState x:Name="Transit">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Visibility)" Storyboard.TargetName="DirectionsWalkingSummaryGrid">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="TransitSummaryGrid">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="DrivingWalking">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Visibility)" Storyboard.TargetName="DirectionsWalkingSummaryGrid">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="TransitSummaryGrid">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.Resources>
                            <x:String x:Key="Delimitter">&#x00A0;</x:String>
                            <x:String x:Key="WordEllipsis">&#x25CB;</x:String>
                            <CollectionViewSource x:Name="itineraryStepViewSource" 
                                                  IsSourceGrouped="True" 
                                                  ItemsPath="Items" />
                             <CollectionViewSource x:Name="TransitSummaryViewSource" />
                            <DataTemplate x:Name="InstructionTemplate">
                                <Grid MinHeight="86">
                                    <Grid Margin="20,10,24,10">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock
                                                x:Name="StepIcon"
                                                Grid.Column="0"
                                                Grid.Row="1"
                                                Text="{Binding Icon}"
                                                FontSize="21"
                                                FontFamily="{StaticResource SymbolFont}"
                                                VerticalAlignment="Top"
                                                HorizontalAlignment="Left"/>

                                        <StackPanel
                                                Grid.Column="1"
                                                Grid.Row="0"
                                                Margin="0,0,0,-2"
                                                Orientation="Horizontal">
                                            <TextBlock
                                                    Text="{Binding Distance}"
                                                    Style="{StaticResource SmallText-SB-None}"/>
                                        </StackPanel>

                                        <RichTextBlock
                                                Grid.Column="1"
                                                Grid.Row="1"
                                                IsTextSelectionEnabled="False"
                                                TextWrapping="Wrap"
                                                Style="{StaticResource MediumText-R-None-RichTextBlock}">
                                            <Paragraph>
                                                <Run Text="{Binding DisplayIndex}"/>
                                                <Run Text=". "/>
                                                <Run Text="{Binding Instruction}"/>
                                            </Paragraph>
                                        </RichTextBlock>

                                        <TextBlock
                                                Grid.Column="1"
                                                Grid.Row="2"
                                                Text="{Binding Hint}"
                                                TextWrapping="Wrap"
                                                VerticalAlignment="Top"
                                                Style="{StaticResource SmallText-SL-None}"/>
                                        <ItemsControl
                                                Grid.Column="1"
                                                Grid.Row="3"
                                                ItemsSource="{Binding Warnings}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="20"/>
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock
                                                            Grid.Column="0"
                                                            Grid.Row="1"
                                                            Text="&#xE047;"
                                                            FontSize="12"
                                                            FontFamily="{StaticResource SymbolFont}"
                                                            VerticalAlignment="Top"
                                                            HorizontalAlignment="Left"/>
                                                        <TextBlock
                                                            Grid.Column="1"
                                                            Grid.Row="0"
                                                            Text="{Binding Message}"
                                                            TextWrapping="Wrap"
                                                            VerticalAlignment="Top"
                                                            Style="{StaticResource SmallText-SL-None}"/>
                                                    </Grid>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </Grid>
                                </Grid>
                            </DataTemplate>
                            <DataTemplate x:Name="WaypointTemplate">
                                <Grid MinHeight="86">
                                    <Grid Margin="20,10,24,10">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock
                                                x:Name="StepIcon"
                                                Grid.Column="0"
                                                Grid.Row="1"
                                                Text="{Binding Icon}"
                                                Foreground="{Binding IconColor}"
                                                FontSize="21"
                                                FontFamily="{StaticResource SymbolFont}"
                                                VerticalAlignment="Top"
                                                HorizontalAlignment="Left"/>
                                        <StackPanel
                                                Grid.Column="1"
                                                Grid.Row="0"
                                                Margin="0,0,0,-2"
                                                Orientation="Horizontal">
                                            <TextBlock
                                                    Text="{Binding Distance}"
                                                    Style="{StaticResource SmallText-SB-None}"/>
                                        </StackPanel>
                                        
                                        <RichTextBlock
                                               Grid.Column="1"
                                               Grid.Row="1"
                                               IsTextSelectionEnabled="False"
                                               TextWrapping="Wrap"
                                               Style="{StaticResource MediumText-R-None-RichTextBlock}">
                                            <Paragraph>
                                                <Run Text="{Binding DisplayIndex}"/>
                                                <Run Text=". "/>
                                                <Run Text="{Binding Instruction}"/>
                                            </Paragraph>
                                        </RichTextBlock>

                                        <TextBlock
                                                Grid.Column="1"
                                                Grid.Row="2"
                                                Text="{Binding Path=Hint}"
                                                TextWrapping="Wrap"
                                                VerticalAlignment="Top"
                                                Style="{StaticResource SmallText-SL-None}"/>
                                    </Grid>
                                </Grid>
                            </DataTemplate>
                            <DataTemplate x:Name="TransitWaypointTemplate">
                                <Grid>
                                    <Grid Margin="20,10,24,10">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock
